{
    "functions": {
      "api/**/*.js": {
        "runtime": "nodejs14.x",
        "maxDuration": 10,
        "memory": 512
      }
    },
    "routes": [
      { "handle": "filesystem" },
      {
        "src": "/index.js",
        "dest": "/api/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/api/$1"
      }
    ]
  }
  